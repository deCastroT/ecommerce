<% include header %>

<br/>
<br/>
<div class="container-fluid">
  <div class=row>
    <div class="col col-md-12">
      <a href="orderLineItems/create" class="btn btn-primary new">
        Create a orderLineItem
      </a>
      <h2>Recent orderLineItem entries</h2>
      <p>There are
        <%= orderLineItems.query.length %> orderLineItems.
          <br/>
      </p>
      <% for (var i = orderLineItems.query.length-1; i >= 0; i--) {   
        orderLineItem = orderLineItems.query[i];   %>
        <div class="panel panel-default">
          <div class="panel-heading"></div>
          <div class="text-muted pull-right"></div>
          <div class="panel-body">
            br/> ID:
            <%=  orderLineItems._id %>
              <br> orderID:
              <%=orderLineItems.orderID%>
                <br/> productQuantity:
                <%=orderLineItems.productQuantity%>
                  <br/> productID:
                  <%=orderLineItems.productID%>
                  <br/>productPrice:
                  <%=orderLineItems.productPrice%>
                    <br/>
                          <br/>
                          <br/>
                          <a href="/orderLineItems/delete/<%= orderLineItems._id  %>" class="btn btn-danger"> Delete </a>
                          <a href="/orderLineItems/details/<%= orderLineItems._id %>" class="btn btn-info"> Details </a>
                          <a href="/orderLineItems/edit/<%= orderLineItems._id %>" class="btn btn-warning"> Edit </a>
          </div>
        </div>
        <% } %>

    </div>
  </div>
</div>